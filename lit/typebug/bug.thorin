.plugin mem;
.plugin core;
.plugin typebug;

.let S1 = (2, (1,3), %typebug.TF32);
.let T1 = %typebug.Tsr S1;

.let S2 = (2, (2,3), %typebug.TF32);
.let T2 = %typebug.Tsr S2;


.con .extern math_test [mem : %mem.M,t1: %mem.Ptr(T1, 0), t2: %mem.Ptr(T2, 0),return : .Cn [%mem.M]] = {
    .let (`mem, t4) = %mem.load(mem, t1);
    .let t3 = %typebug.broadcast(S1, S2) t4;
    .let `mem = %mem.store(mem, t2, t3);
    return mem
};

